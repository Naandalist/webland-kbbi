{
	"routes": [
		{
			"src": "/(.*)",
			"headers": [
				{
					"key": "X-Robots-Tag",
					"value": "noindex, nofollow"
				}
			]
		},
		{
			"src": "/404",
			"status": 404,
			"dest": "/404.astro"
		},
		{
			"src": "/privacy",
			"dest": "/privacy.astro"
		},
		{
			"src": "/terms",
			"dest": "/terms.astro"
		}
	]
}